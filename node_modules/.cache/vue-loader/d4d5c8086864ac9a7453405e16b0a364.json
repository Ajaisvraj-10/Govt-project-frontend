;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"header.vue","sourceRoot":"src/views/app/pages","sourcesContent":["  \r\n<template>\r\n  <div class=\"main-content\">\r\n    <header class=\"flex-align px-3\">\r\n      <img src=\"/images/logo.png\" width=\"120\">\r\n      <!--<img src=\"/images/logo.png\" width=\"120\" v-if=\"logo_tab=='logo1'\" @click=\"logo_tab='logo2'\">\r\n      <img src=\"/images/logol.png\" width=\"120\" v-if=\"logo_tab=='logo2'\" @click=\"logo_tab='logo3'\">\r\n      <img src=\"/images/logol1.png\" width=\"120\" v-if=\"logo_tab=='logo3'\" @click=\"logo_tab='logo4'\">\r\n      <img src=\"/images/logol2.png\" width=\"120\" v-if=\"logo_tab=='logo4'\" @click=\"logo_tab='logo5'\">\r\n      <img src=\"/images/logol4.png\" width=\"120\" v-if=\"logo_tab=='logo5'\" @click=\"logo_tab='logo1'\">-->\r\n\r\n\r\n\r\n      <div class=\"d-flex g3\">\r\n        <button  class=\"btn-violet d-flex px-2 plan_btn\" v-b-modal.app-modal>\r\n          <img src=\"/images/plan.png\" style=\"width: 15px;margin-right: 8px;align-self: center;\">Apps\r\n        </button>\r\n        <div class=\"navbar-toggle2r open_side_menu\" type=\"button\" v-b-modal.profile-modal>\r\n          <img class=\"profile_icon\" :src=\"profile.profile_picture?profile.profile_picture:'https://leadfinder.live/assets/img/user_1.png'\" style=\"width: 36px; height: 36px; border-radius: 50%; \">\r\n        </div>\r\n      </div>\r\n    </header>\r\n\r\n    <div style=\"position: absolute;top: -60px;\">\r\n      <iframe src=\"https://checkout.stripe.com/c/pay/\" style=\"height: 50px;width: 100%;\"></iframe>\r\n    </div>\r\n\r\n    \r\n    <b-modal id=\"app-modal\" size=\"sm\"  hide-header hide-footer>\r\n        <main class=\"lead-main  mt-0\" style=\"height: fit-content;background-color:initial;padding: 1.3rem;gap:1rem;\">\r\n          <div class=\"modal-header w-100 d-flex justify-content-end px-1 pb-3 pt-1 border-0\" >\r\n            <img src=\"/images/close.png\" width=\"14\" style=\"opacity:.7;\" @click=\"$bvModal.hide('app-modal')\">\r\n          </div>\r\n          <div class=\"app-item\" @click=\"redirectTo('lead_finding')\">\r\n            <div class=\"flex-align g2 w-100\">\r\n              <div class=\"d-flex g3\">\r\n                <img src=\"/images/lead3.png\">\r\n                <div>\r\n                  <h3>Lead Finding</h3>\r\n                  <h6 class=\"\">A tool for discovering potential business opportunities</h6>\r\n                </div>\r\n              </div>\r\n              <img src=\"/images/toggle2.png\" class=\"toggle deactive-toggle\">\r\n              <img src=\"/images/toggle2-active.png\" class=\"toggle active-toggle\">\r\n            </div>\r\n            \r\n\r\n          </div>\r\n          \r\n          <div class=\"app-item\" @click=\"redirectTo('crm')\">\r\n            <div class=\"flex-align g2 w-100\">\r\n              <div class=\"d-flex g3\">\r\n                <img src=\"/images/crm3.png\">\r\n                <div>\r\n                  <h3>CRM</h3>\r\n                  <h6 class=\"\">Keep track of and control your business leads using the Leadfinder CRM tool</h6>\r\n                </div>\r\n              </div>\r\n              <img src=\"/images/toggle2.png\" class=\"toggle deactive-toggle\">\r\n              <img src=\"/images/toggle2-active.png\" class=\"toggle active-toggle\">\r\n            </div>\r\n            \r\n\r\n          </div>\r\n          <!-- <div class=\"app-item\" @click=\"redirectTo('email_marketing')\">\r\n            <div class=\"flex-align g2 w-100\">\r\n              <div class=\"d-flex g3\">\r\n                <img src=\"/images/email3.png\">\r\n                <div>\r\n                  <h3>Email Marketing</h3>\r\n                  <h6 class=\"\">Create and send promotional emails to the leads in your CRM</h6>\r\n                </div>\r\n              </div>\r\n              <img src=\"/images/toggle2.png\" class=\"toggle deactive-toggle\">\r\n              <img src=\"/images/toggle2-active.png\" class=\"toggle active-toggle\">\r\n            </div>\r\n           \r\n\r\n          </div>\r\n          \r\n          <div class=\"app-item\" @click=\"redirectTo('news')\">\r\n            <div class=\"flex-align g2 w-100\">\r\n              <div class=\"d-flex g3\">\r\n                <img src=\"/images/newsletter3.png\">\r\n                <div>\r\n                  <h3>AI Newsletter</h3>\r\n                  <h6 class=\"\">A newsletter generation tool powered by ChatGPT's AI technology</h6>\r\n                </div>\r\n              </div>\r\n              <img src=\"/images/toggle2.png\" class=\"toggle deactive-toggle\">\r\n              <img src=\"/images/toggle2-active.png\" class=\"toggle active-toggle\">\r\n            </div>\r\n            \r\n\r\n          </div> -->\r\n        </main>\r\n    </b-modal>\r\n   \r\n    <b-modal id=\"profile-modal\" hide-header hide-footer>\r\n      <div class=\"modal-header\" style=\"position:absolute;right:0px;top:0px;z-index:99;border: none;\">\r\n        <img src=\"/images/close2.png\" width=\"14\" style=\"opacity:.7;\" @click=\"$bvModal.hide('profile-modal')\">\r\n      </div>\r\n      <div style=\"\r\n          height: 70vh;\r\n          position: absolute;\r\n          align-items: center;\r\n          padding-left: 100px;\r\n          right: -120px;\r\n          z-index: 0;\r\n          padding-top: 100px;\r\n          top: 0;\r\n          background-image: linear-gradient(#ffffff, #e9e9e9);\">\r\n        <img src=\"https://leadfinder.live/assets/img/iphone.png\" style=\"opacity: .8;\">\r\n      </div>\r\n      <ul class=\"m-0 ml-lg-auto p-0 side_menu p-lg-0\" style=\"min-height:40vh;max-height:90vh;overflow:auto;z-index: 9;position: relative;\">\r\n       \r\n        <li class=\"nav-item pl-2 d-flex justify-content-between mt-1\">\r\n          <div class=\"d-flex align-items-center\">\r\n            <img class=\"profile_pic\" :src=\"profile.profile_picture?profile.profile_picture:'https://leadfinder.live/assets/img/user_1.png'\" style=\"width: 46px; height: 46px; border-radius: 50%; margin-top: 2px;\">\r\n            <div class=\"px-3\" style=\"max-width: 260px; overflow-wrap: break-word;\">\r\n              <h5 class=\" username text-18 mb-1\" style=\"font-weight: 500;\">{{profile.name}}</h5>\r\n              <p class=\"m-0 useremail\" style=\"font-size: 14px;\">{{profile.email}}</p>\r\n            </div>\r\n          </div>\r\n        </li>\r\n\r\n        <hr class=\"mt-4 mb-2\">\r\n        <li class=\"\" >\r\n          <a class=\"\">{{current_plan.plan}}</a>\r\n          <div class=\"d-flex g1\"  style=\"align-items: flex-start;\">\r\n            <a class=\"fff text-13 card\" style=\"line-height:20px;padding: .8rem;white-space: nowrap;\">Lead Finder <span class=\" badge badge-dark mt-1 text-14\">{{nFormatter(current_plan.leads-plan_info.used_leads,1)}}</span></a>\r\n            <a class=\"fff text-13 card\" style=\"line-height:20px;padding: .8rem;white-space: nowrap;\">CRM <span class=\" badge badge-dark mt-1 text-14\">{{nFormatter(current_plan.crm-plan_info.used_crm,1)}}</span></a>\r\n            <a class=\"fff text-13 card\" style=\"line-height:20px;padding: .8rem;white-space: nowrap;filter: blur(1.5px);\">Email Credits <span class=\" badge badge-dark mt-1 text-14\"><!--{{nFormatter(current_plan.emails-plan_info.used_emails,1)}}-->Coming</span></a>\r\n          </div>\r\n         \r\n        </li>\r\n        \r\n        <hr class=\"mt-3 mb-2\">\r\n   \r\n        <li class=\"nav-item\">\r\n            <a class=\"nav-link plan_btn\" ><i class=\"fa fa-question-circle\" aria-hidden=\"true\"></i> Support</a>\r\n        </li>\r\n\r\n        <li class=\"nav-item\" @click=\"free_plan=false\" v-b-modal.plans>\r\n            <a class=\"nav-link plan_btn\"> <i class=\"fa fa-wrench\" aria-hidden=\"true\"></i> Upgrade Plan</a>\r\n        </li>\r\n\r\n        <li class=\"nav-item\" v-b-modal.account>\r\n            <a class=\"nav-link plan_btn\"> <i class=\"fa fa-user\" aria-hidden=\"true\"></i> Account</a>\r\n        </li>\r\n       \r\n        <li class=\"nav-item\">\r\n            <a class=\"nav-link \" href=\"/maps.html?logout=true\"><i class=\"fas fa-sign-out-alt\"></i> Sign out</a>\r\n        </li>\r\n        <!-- <li class=\"nav-item\" v-b-modal.em-modal>\r\n            <a class=\"nav-link \" ><i class=\"fas fa-cog\"></i> Shopify 1</a>\r\n        </li>\r\n        <li class=\"nav-item\" v-b-modal.em-modal2>\r\n            <a class=\"nav-link \" ><i class=\"fas fa-cog\"></i> Shopify 2</a>\r\n        </li> -->\r\n       \r\n      </ul>\r\n\r\n    </b-modal>\r\n\r\n    <b-modal id=\"profile\" hide-header hide-footer>\r\n      <main class=\"text-center\">\r\n        <div class=\"proflie-page\" style=\"height: 100%;\" >\r\n          <div class=\"flex-align my-1\" >\r\n            <div style=\"width:30px;\">\r\n              <img src=\"/images/back.png\" width=\"26\" style=\"opacity:.7;\"  @click=\"crnt_tab_profile='profile'\"  \r\n              v-if=\"crnt_tab_profile=='profile-edit' || crnt_tab_profile=='change-password'\"\r\n              >\r\n            </div>\r\n            <h2 class=\"text-center text-22\">Profile</h2>\r\n            <div style=\"width:30px;\">\r\n              <img src=\"/images/close2.png\" width=\"14\" style=\"opacity:.7;\" @click=\"$bvModal.hide('profile')\">\r\n            </div>\r\n          </div>\r\n          <section class=\"profile-sec px-4\" v-if=\"crnt_tab_profile=='profile'\">\r\n            \r\n            <div class=\"change-dp\" >\r\n              <img class=\"profile_pic\" :src=\"profile.profile_picture?profile.profile_picture:'https://leadfinder.live/assets/img/user_1.png'\" style=\"width: 80px; height: 80px; border-radius: 50%; margin-top: 2px;\">\r\n              <h6 class=\"text-primary\">{{profile.name}}</h6>\r\n            </div>\r\n            <table class=\" w-100 border-none\">\r\n              <tbody>\r\n                <tr><th>Email</th><th>:</th><td class=\"user_email\">{{profile.email}}</td></tr>\r\n                <tr><th>Phone</th><th>:</th><td class=\"user_phone\">{{profile.phone}}</td></tr>\r\n                <tr><th>Current Plan</th><th>:</th><td class=\"user_plan\">{{current_plan.plan}}  <span class=\"badge badge-danger\" v-if=\"current_plan.expired\">Expired</span></td></tr>\r\n                <tr><th>Used Leads</th><th>:</th><td class=\"user_leads\">{{parseInt(plan_info.used_leads)}}/{{current_plan.leads==-99?'Unlimited':current_plan.leads}}</td></tr>\r\n              </tbody>\r\n            </table>\r\n            <div class=\"text-center mt-4 w-100\">\r\n              <a  class=\"text-primary m-0\" @click=\"crnt_tab_profile='profile-edit'\">Edit Profile</a>\r\n            </div>\r\n            \r\n          </section>\r\n          <section class=\"profile-sec px-4\" v-if=\"crnt_tab_profile=='profile-edit'\">\r\n            <form id=\"profile_form\" style=\"margin: 0;\">\r\n              <div class=\"change-dp\" >\r\n                <img class=\"profile_pic\" :src=\"profile.profile_picture?profile.profile_picture:'https://leadfinder.live/assets/img/user_1.png'\" style=\"width: 80px; height: 80px; border-radius: 50%; margin-top: 2px;\">\r\n                <h6 class=\"text-primary\">Change Profile Photo</h6>\r\n                <input type=\"file\" name=\"image\" @change=\"updateProfilePic()\">\r\n              </div>\r\n            </form>\r\n            <div>\r\n              <button class=\"btn-input btn-block my-4\">  \r\n                <img src=\"https://leadfinder.live/assets/img/user.png\" style=\"width: 13px;\" class=\"ml-4 mr-2\">\r\n                <input type=\"text\" name=\"name\" v-model=\"profile.name\">\r\n              </button>\r\n              <button class=\"btn-input btn-block my-4\">  \r\n                <img src=\"https://leadfinder.live/assets/img/mail.png\" style=\"width: 13px;\" class=\"ml-4 mr-2\">\r\n                <input type=\"email\" name=\"email\" id=\"email\" v-model=\"profile.email\" readonly=\"\">\r\n              </button>\r\n              <button class=\"btn-input btn-block my-4\">  \r\n                <img src=\"https://leadfinder.live/assets/img/phone-2.png\" style=\"width: 13px;\" class=\"ml-4 mr-2\">\r\n                <input type=\"number\" name=\"phone\" v-model=\"profile.phone\">\r\n              </button>\r\n              <button class=\"btn btn-info btn-block my-4 py-3 text-18\" @click=\"updateUser()\">Save</button>\r\n              <div class=\"text-center w-100\">\r\n                <a  class=\"text-primary m-0\" @click=\"crnt_tab_profile='change-password'\">Change Password?</a>\r\n              </div>\r\n            </div>\r\n          </section>\r\n          <section class=\"profile-sec px-4\" v-if=\"crnt_tab_profile=='change-password'\">\r\n            \r\n            <div>\r\n              <div class=\"mx-auto my-2\" style=\"background-image: url(https://leadfinder.live/assets/img/password.jpg);background-position: center;background-repeat: no-repeat; background-size: contain; width: 240px;height: 140px;\">\r\n          \r\n              </div>\r\n              <h3 class=\"font-weight-bold my-4\">Edit Password</h3>\r\n              <button class=\"btn-input btn-block my-4\">  \r\n                <img src=\"https://leadfinder.live/assets/img/lock.png\" style=\"width: 13px;\" class=\"ml-4 mr-2\">\r\n                <input type=\"password\" name=\"password\" id=\"password\" placeholder=\"New Password\">\r\n              </button>\r\n              <button class=\"btn-input btn-block my-4\">  \r\n                <img src=\"https://leadfinder.live/assets/img/lock.png\" style=\"width: 13px;\" class=\"ml-4 mr-2\">\r\n                <input type=\"password\" name=\"password\" id=\"password\" placeholder=\"Confirm  Password\">\r\n              </button>\r\n              <button class=\"btn btn-info btn-block my-4 py-3 text-18\" @click=\"updatePassword()\">Submit</button>\r\n              \r\n            </div>\r\n          </section>\r\n        </div>\r\n      </main>\r\n    </b-modal>\r\n    <b-modal id=\"account\" hide-header hide-footer>\r\n      <main class=\"text-center\">\r\n        <div class=\"proflie-page\" style=\"height: 100%;\" >\r\n          <div class=\"flex-align my-1\" >\r\n            <div style=\"width:30px;\">\r\n            \r\n            </div>\r\n            <h2 class=\"text-center text-22\">Account</h2>\r\n            <div style=\"width:30px;\">\r\n              <img src=\"/images/close2.png\" width=\"14\" style=\"opacity:.7;\" @click=\"$bvModal.hide('account')\">\r\n            </div>\r\n          </div>\r\n          <section class=\"profile-sec py-4\" style=\"min-height:55vh;max-height:80vh;overflow:auto;\">\r\n            <div class=\"card mb-3\" style=\"box-shadow:none;\" v-b-modal.profile>\r\n              <div class=\"flex-align\">\r\n                <h4>Profile</h4>\r\n                <i class=\"fa fa-angle-right\"></i>\r\n              </div>\r\n            </div>\r\n            <div class=\"card mb-3\" style=\"box-shadow:none;\" v-b-modal.cancel-account>\r\n              <div class=\"flex-align\">\r\n                <h4>Cancel Plan</h4>\r\n                <i class=\"fa fa-angle-right\"></i>\r\n              </div>\r\n            </div>\r\n            <hr class=\"my-4\">\r\n            <div>\r\n              <button class=\"btn btn-dark btn-block text-18 py-2\" v-b-modal.delete-account>Delete Account</button>\r\n            </div>\r\n          </section>\r\n          \r\n       \r\n        </div>\r\n      </main>\r\n    </b-modal>\r\n    \r\n    <b-modal id=\"plans\" hide-header hide-footer>\r\n      \r\n      <div class=\"plan_loading\" v-if=\"$root.lock_btn\">\r\n        <div class=\"loading_wrap2\" id=\"loading_wrap2\">\r\n          <div id=\"loading-bar-spinner\" class=\"spinner\"><div class=\"spinner-icon\"></div></div>\r\n        </div>\r\n      </div>\r\n      <div class=\"\" style=\"position:absolute;right:15px;top:15px;z-index:99;\">\r\n        <img src=\"/images/close2.png\" width=\"14\" style=\"opacity:.7;\" @click=\"$bvModal.hide('plans')\">\r\n      </div>\r\n      <main class=\"text-center\" style=\"position: relative;\">\r\n        <div class=\"other_pages plan pb-1\" style=\"height: 100%;height: 75vh;overflow:auto;\" >\r\n            <div class=\"membership container mt-4\" >\r\n              <div class=\"card-2 row  m-auto\" v-for=\"(plan,key) in plans\" v-if=\"(free_plan && plan.type=='Trial') || (!free_plan && plan.type!='Trial')\">\r\n              <!--<h2 class=\" mb-3 text-center w-100\">{{plan.plan}}</h2>-->\r\n              <h2 class=\" mb-3 text-center w-100\">Pricing</h2>\r\n\r\n                <div class=\"col-12 px-0\" >\r\n                    <div class=\"card-2__price text-center p-4\" style=\"width: 100%;margin-bottom: 10px;background: #fff;color: #000;box-shadow: 0px 0px 0px 3px #000;border: none;\">\r\n                    <div class=\"flex-align2 g2\"  style=\"border-bottom: 1px solid #000;font-weight: 400;\">\r\n                      \r\n                      <div class=\"\" style=\"margin-top:-10px;\">\r\n                        <span class=\"strikethrough\" v-if=\"plan.type!='Trial'\">\r\n                          <span class=\"\" style=\"font-size: 30px;\">$</span>\r\n                          <span class=\"\" style=\"font-size: 30px;\">{{plan.show_amount}}</span> \r\n                        </span>\r\n                        <span v-if=\"plan.type!='Trial'\">\r\n                          <span class=\"text-danger\" style=\"font-size: 30px;\">$</span>\r\n                          <span class=\"text-danger\" style=\"font-size: 30px;\">{{plan.amount}}</span> \r\n                        </span>\r\n                        <span class=\"\" v-if=\"plan.type=='Trial'\">\r\n                          <span class=\"text-danger\" style=\"font-size: 30px;\">$</span>\r\n                          <span class=\"text-danger\" style=\"font-size: 30px;\">{{plan.amount}}</span> \r\n                        </span>\r\n                        \r\n\r\n                        <span class=\"m-0\" style=\"font-size: 12px;\">/ {{plan.id==2?'Week':'Year'}}</span>                    \r\n                      </div>\r\n                    </div>\r\n                    <ul class=\"card-2__list  align-self-center\" style=\"color: #706e6e;\">\r\n                      <li class=\"card-2__list__item\"><span class=\"pr-2\" style=\"font-weight:700;font-size:18px;color: #000;\"> {{nFormatter(plan.leads)}} </span>Leads/{{plan.lead_duration=='Day'?plan.duration:''}}{{ plan.lead_duration }}{{ checkMult(plan.duration) }}</li>\r\n                      <!-- <li class=\"card-2__list__item\">{{plan.emails}} Email credits</li> -->\r\n                      <li class=\"card-2__list__item\">Unlimited Locations</li>\r\n                      <li class=\"card-2__list__item\">Unlimited Business Category</li>\r\n                      <li class=\"card-2__list__item\"><span style=\"font-weight:700;font-size:17px;color: #000;padding-right: 5px;\">{{nFormatter(plan.crm)}} </span> CRM Contacts</li>\r\n                      <!-- <li class=\"card-2__list__item\">Email marketing Tool</li> -->\r\n                      <!-- <li class=\"card-2__list__item\">Domain Verification</li> -->\r\n                    </ul>\r\n                    </div>\r\n                </div>\r\n                <div class=\"plan-btn\" style=\"width: 100%;\">\r\n                  <a class=\"btn btn-block choose_plan\" :id=\"'plan_btn'+plan.id\" @click=\"choosePlan(plan.id)\">\r\n                    {{plan.id==current_plan.id?'Subscribed':'Buy Now'}}\r\n                  </a>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <!-- <div class=\"switch-main mt-4\" v-if=\"current_plan.type=='Free'\">\r\n              <label class=\"toggle2\">\r\n              <input class=\"toggle2-checkbox\" type=\"checkbox\" v-model=\"free_plan\">\r\n              <div class=\"toggle2-switch\"></div>\r\n              <span class=\"toggle2-label\">I want 7 Days Trial</span>\r\n            </label>\r\n            </div> -->\r\n        </div>\r\n      </main>\r\n        \r\n    </b-modal>\r\n    <b-modal id=\"delete-account\" class=\"delete-modals\" hide-header hide-footer>\r\n      <div class=\"modal-bar\"></div>\r\n      <main class=\"\">\r\n        <div class=\"proflie-page mt-4\" style=\"height: 100%;\" >\r\n          <div class=\"flex-align text-17\" style=\"line-height:20px;\">Are you sure you want to delete your account?\r\n            <div style=\"width:30px;\">\r\n              <img src=\"/images/close2.png\" width=\"14\" style=\"opacity:.7;\" @click=\"$bvModal.hide('delete-account')\">\r\n            </div>\r\n          </div>\r\n          <hr class=\"my-2\">\r\n          <section class=\"profile-sec pt-3 pb-0\">\r\n            <p class=\"text-14 text-light\" style=\"line-height:20px;\">All your data will be completely deleted and you won't be able to recover it.</p>\r\n          </section>\r\n          <hr class=\"my-3\">\r\n          <div class=\"m-0 g2 row justify-content-center\">\r\n            <button class=\"btn btn-outline-dark col-5  text-18 py-2\" @click=\"$bvModal.hide('delete-account')\">Cancel</button>\r\n            <button class=\"btn btn-dark  text-18 col-5 py-2\">Confirm</button>\r\n\r\n          </div>\r\n       \r\n        </div>\r\n      </main>\r\n    </b-modal>\r\n    <b-modal id=\"cancel-account\" class=\"delete-modals\" hide-header hide-footer>\r\n      <div class=\"modal-bar\"></div>\r\n      <main class=\"\">\r\n        <div class=\"proflie-page mt-4\" style=\"height: 100%;\" >\r\n          <div class=\"flex-align text-17\" style=\"line-height:20px;\">Are you sure you want to cancel your plan?\r\n            <div style=\"width:30px;\">\r\n              <img src=\"/images/close2.png\" width=\"14\" style=\"opacity:.7;\" @click=\"$bvModal.hide('cancel-account')\">\r\n            </div>\r\n          </div>\r\n          <hr class=\"my-2\">\r\n          <section class=\"profile-sec pt-3 pb-0\">\r\n            <p class=\"text-14 text-light\" style=\"line-height:20px;\">You can cancel your Premium plan.</p>\r\n          </section>\r\n          <hr class=\"my-3\">\r\n          <div class=\"m-0 g2 row justify-content-center\">\r\n            <button class=\"btn btn-outline-dark col-5  text-18 py-2\" @click=\"$bvModal.hide('cancel-account')\">Cancel</button>\r\n            <button class=\"btn btn-dark  text-18 col-5 py-2\">Confirm</button>\r\n\r\n          </div>\r\n       \r\n        </div>\r\n      </main>\r\n    </b-modal>\r\n\r\n    <b-modal id=\"em-modal\" hide-header hide-footer>\r\n        <div class=\"modal-header w-100 d-flex justify-content-end px-1 pb-3 pt-1 border-0\" >\r\n            <img src=\"/images/close.png\" width=\"14\" style=\"opacity:.7;\" @click=\"$bvModal.hide('em-modal')\">\r\n        </div>\r\n        <div class=\"trial-modal\">\r\n          <main class=\"lead-main text-center\">\r\n            <h1>Start for free, then enjoy <span class=\"text-primary\">$1/month</span> for 3 months</h1>\r\n            <h5>Try Leadfinder free for 3 days, no credit card required</h5>\r\n            <button class=\"btn\">Start free trial</button>\r\n            <p>By entering your email, you agree to recieve marketing emails from leadfinder.</p>\r\n          </main>\r\n        </div>\r\n    </b-modal>\r\n    <b-modal id=\"em-modal2\" size=\"sm\"   hide-header hide-footer>\r\n        <div class=\"backdrop-close\"></div>\r\n        <div class=\"modal-header w-100 d-flex justify-content-end  p-4 pb-0 border-0\" >\r\n            <img src=\"/images/close.png\" width=\"14\" style=\"opacity:.7;\" @click=\"$bvModal.hide('em-modal2')\">\r\n        </div>\r\n        <div class=\"trial-modal\">\r\n          <main class=\"lead-main text-center\">\r\n            <h1>Start for free, then enjoy <span class=\"text-primary\">$1/month</span> for 3 months</h1>\r\n            <h5>Try Leadfinder free for 3 days, no credit card required</h5>\r\n            <button class=\"btn\">Start free trial</button>\r\n            <p>By entering your email, you agree to recieve marketing emails from leadfinder.</p>\r\n          </main>\r\n        </div>\r\n    </b-modal>\r\n  </div>\r\n</template>\r\n<script>\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Campaigns\"\r\n  },\r\n  data() {\r\n    return {\r\n      profile:{},\r\n      current_plan:{},\r\n      plan_info:{used_crm: 0,used_emails: 0,used_leads: 159,used_newsletters: 0},\r\n      plans:[],\r\n      selected_plan:{},\r\n      crnt_tab: 'tab1',\r\n      crnt_tab_profile: 'profile',\r\n      free_plan:false,\r\n      logo_tab: 'logo1',\r\n\r\n    };\r\n  },\r\n  mounted(){\r\n    if(this.$route.query.auth_token){\r\n      this.$root.auth_token = this.$route.query.auth_token\r\n      localStorage.setItem(\"auth_token\",this.$root.auth_token)\r\n    }\r\n    if(this.$root.auth_token){\r\n      this.getProfile()\r\n      this.getPlans()\r\n    }\r\n  },\r\n  methods: {\r\n    getProfile() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.auth_token);\r\n      fetch(this.api_url+'/employee/user_data/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        if(jsonData.success){\r\n          this.current_plan = jsonData.current_plan.plan_details\r\n          this.plan_info = jsonData.current_plan\r\n          this.profile = jsonData.user\r\n        }\r\n      })\r\n    },\r\n    getPlans(){\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.auth_token);\r\n      fetch(this.api_url+'/employee/plans/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((data) => {\r\n        this.plans = data\r\n      })\r\n    },\r\n    updateProfilePic(e){\r\n      var img_file = e.target.files[0];\r\n      var img_size = img_file.size/1000\r\n      if(img_size>850){\r\n        this.showAlert('maximum size of image is 850KB','red',5000)\r\n        e.target.value = ''\r\n        this.profile.profile_picture = ''\r\n        return false\r\n      }\r\n      \r\n      this.profile.profile_picture= URL.createObjectURL(img_file);\r\n\r\n      var headers = new Headers();\r\n      var formdata = new FormData(document.getElementById('profile_form'))\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.auth_token);\r\n      fetch(this.api_url+'/employee/user_data/', {\r\n          method : 'put',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((data) => {\r\n        if(data.success){\r\n          this.showAlert(data.message)\r\n        }\r\n      })\r\n    },\r\n    updateUser(){\r\n      var headers = new Headers();\r\n      var formdata = new FormData()\r\n      formdata.append('name', this.profile.name)\r\n      formdata.append('phone', this.profile.phone)\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.auth_token);\r\n      fetch(this.api_url+'/employee/user_data/', {\r\n          method : 'put',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((data) => {\r\n        if(data.success){\r\n          this.showAlert(data.message)\r\n        }\r\n      })\r\n    },\r\n    updatePassword(){\r\n      var password = document.getElementById('password').value\r\n      var password2 = document.getElementById('password2').value\r\n      if(password=='' || password != password2){\r\n        this.showAlert('Woops: Passwords are does not match!')\r\n        return false\r\n      }\r\n      var headers = new Headers();\r\n      var formdata = new FormData()\r\n      formdata.append('password',password)\r\n      formdata.append('password1',password2)\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.auth_token);\r\n      fetch(this.api_url+'/employee/user_data/', {\r\n          method : 'post',\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((data) => {\r\n        if(data.success)\r\n        {\r\n          this.showAlert(data.message)\r\n        }else{\r\n          this.showAlert(data.message)\r\n        }\r\n      })\r\n    },\r\n    choosePlan(plan_id){\r\n      var method = 'POST'\r\n      var params = ''\r\n      if(this.profile.payment_method){\r\n        method = 'PUT'\r\n        params = this.profile.payment_method+'/'\r\n      }\r\n      if(this.$root.lock_btn){\r\n        return false\r\n      }\r\n      this.$root.lock_btn = true\r\n      document.getElementById('plan_btn'+plan_id).innerHTML = 'Please Wait..'\r\n      var headers = new Headers();\r\n      var formdata = new FormData()\r\n      formdata.append('plan_id',plan_id)\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.auth_token);\r\n      fetch(this.api_url+'/employee/stripe_payments/'+params, {\r\n          method : method,\r\n          headers: headers,\r\n          body: formdata,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((data) => {\r\n        if(data.success)\r\n        {\r\n          if(data.payment_url){\r\n            // $('body').html(data.form)\r\n            location.href=data.payment_url\r\n          }else{\r\n            this.$root.lock_btn = false\r\n            document.getElementById('plan_btn'+plan_id).innerHTML = 'Subscribed'\r\n            $('.plan_btn'+plan_id).html('Subscribed')\r\n          }\r\n        }else{\r\n          this.$root.lock_btn = false\r\n          document.getElementById('plan_btn'+plan_id).innerHTML = 'Subscribe Now'\r\n        }\r\n      })\r\n    },\r\n    redirectTo(change_page){\r\n      if(change_page=='email_marketing'){\r\n        this.$router.push('/campaigns')\r\n      }\r\n      if(change_page=='crm'){\r\n        this.$router.push('/leads')\r\n      }\r\n      if(change_page=='lead_finding'){\r\n        location.href = '/maps.html'\r\n      }\r\n      if(change_page=='news'){\r\n        this.$router.push('/news-letters')\r\n      }\r\n      this.$bvModal.hide('app-modal')\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n@import './style/style.css';\r\n</style>\r\n"]}]}