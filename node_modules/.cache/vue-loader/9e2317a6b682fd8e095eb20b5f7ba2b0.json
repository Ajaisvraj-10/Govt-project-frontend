\r\n      try {\r\n        const response = await fetch(\"http://127.0.0.1:8000/api/cropscategory/\");\r\n        const data = await response.json();\r\n        this.categories = data;\r\n      } catch (error) {\r\n        \r\n      }\r\n    },\r\n    async fetchProducts() {\r\n      try {\r\n        const response = await fetch(\"http://127.0.0.1:8000/api/cropsproduct/\");\r\n        const data = await response.json();\r\n        this.products = data;\r\n      } catch (error) {\r\n      \r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchCategories();\r\n    this.fetchProducts();\r\n    this.fetchPlants();\r\n  }\r\n};\r\n</script>\r\n\r\n\r\n<style scoped>\r\n.add-plant {\r\n  max-width: 600px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n}\r\n\r\n.label {\r\n  margin-bottom: 5px;\r\n}\r\n\r\n.input, .select {\r\n  padding: 8px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  margin-bottom: 10px;\r\n}\r\n\r\n.select {\r\n  width: 100%;\r\n}\r\n\r\n.button {\r\n  padding: 8px 12px;\r\n  background-color: #007bff;\r\n  color: white;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n}\r\n\r\n.button:hover {\r\n  background-color: #0056b3;\r\n}\r\n\r\n.plant-list {\r\n  list-style-type: none;\r\n  padding: 0;\r\n}\r\n\r\n.plant-item {\r\n  padding: 5px;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  margin-bottom: 5px;\r\n}\r\n</style>"]}]}