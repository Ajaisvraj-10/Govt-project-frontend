 lead:{id:null, email:[]},\r\n      form_data:{name:'',id:null, activity:null},\r\n      new_email:'',\r\n      background_start:true,\r\n      lastScrollPosition: 0,\r\n      lead_logs:[],\r\n    };\r\n  },\r\n   components:{\r\n      MainHeader,\r\n      MainCrmFooter,\r\n\r\n    },\r\n  mounted(){\r\n    var self = this\r\n    document.getElementsByClassName(\"lead-main\")[0].addEventListener(\"scroll\", () => {\r\n        self.handleScroll()\r\n    });\r\n    if(this.$route.params.id){\r\n      this.lead.id = this.$route.params.id\r\n      this.getLead()\r\n    }\r\n  },\r\n  methods: {\r\n    getLead() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.auth_token);\r\n      fetch(this.api_url+'/employee/leads/'+this.lead.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.lead = jsonData\r\n        this.getLeadLogs()\r\n      })\r\n    },\r\n    updateLead(type='status', content) {\r\n      var headers = new Headers();\r\n      var formdata = new FormData(this.$refs['leadActivityForm'])\r\n      if(this.$root.lock_btn){\r\n        return false\r\n      }\r\n      this.$root.lock_btn = true\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.auth_token);\r\n      formdata.append('type',type)\r\n      if(type=='status'){\r\n        formdata.append('lead_status',content)\r\n        this.lead.lead_status = content\r\n      }\r\n      if(type=='email'){\r\n        formdata.append('email',content)\r\n      }\r\n      fetch(this.api_url+'/employee/leads/'+this.lead.id+'/', {\r\n          method : 'PUT',\r\n          body : formdata,\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.$root.lock_btn = false\r\n        this.$refs.emai