n        </router-link>\r\n      </div> \r\n        <p class=\"text-center text-dark\">Copyright (C) 2023. All rihts reserved Capcee</p>\r\n    </main>\r\n    <div>\r\n      <b-modal id=\"aws-verify-modal\" hide-header hide-footer>\r\n        <div class=\"modal-bar\"></div>\r\n        <div class=\"edit-modal-body2 text-center py-3 mt-3\" style=\"row-gap:25px;\">\r\n          <h1>Verfy Your Email</h1>\r\n          <p>{{ this.$root.userInfo.email }}</p>\r\n          <div>\r\n            Lorem ipsum dolor sit amet consectetur adipisicing elit. Perspiciatis commodi labore ad consequuntur ducimus molestiae\r\n          </div>\r\n        </div>\r\n        <div class=\"footer-modal\">\r\n          <button class=\"btn btn-info px-4 py-2\" @click=\"verifyEmail()\">Verify Now</button>\r\n        </div>\r\n      </b-modal>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n\r\nexport default {\r\n    metaInfo: {\r\n        title: \"Home\"\r\n    },\r\n    data() {\r\n        return {};\r\n    },\r\n    mounted() {\r\n      if(this.$route.query.auth_token){\r\n        this.$root.auth_token = this.$route.query.auth_token\r\n        localStorage.setItem(\"auth_token\",this.$root.auth_token)\r\n      }\r\n      if(this.$root.auth_token){\r\n        this.getUser2()\r\n      }\r\n    },\r\n    methods: {\r\n      getUser2() {\r\n        var headers = new Headers();\r\n        headers.append(\"Authorization\", \"Bearer \"+this.$root.auth_token);\r\n        fetch(this.api_url+'/employee/user_data/', {\r\n            method : 'get',\r\n            headers: headers,\r\n        })\r\n        .then((response) => {\r\n            return response.json()\r\n        })\r\n        .then((jsonData) => {\r\n          if(jsonData.success){\r\n            this.$root.userInfo = jsonData.user\r\n            this.$root.userId = jsonData.user.id\r\n            if(!this.$root.userInfo.aws_verification){\r\n              this.$bvModal.show('aws-verify-modal')\r\n            }\r\n          }\r\n        })\r\n      },\r\n      verifyEmail() {\r\n        var headers = new Headers();\r\n        var formdata = new FormData()\r\n        headers.append(\"Authorization\", \"Bearer \"+this.$root.auth_token);\r\n        fetch(this.api_url+'/employee/verify_mail_aws/', {\r\n            method : 'POST',\r\n            body : formdata,\r\n            headers: headers,\r\n        })\r\n        .then((response) => {\r\n            return response.json()\r\n        })\r\n        .then((jsonData) => {\r\n          if(jsonData.success){\r\n            this.$bvModal.hide('aws-verify-modal')\r\n          }\r\n        })\r\n      },\r\n    },\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n@import './style/style.css';\r\n  \r\n</style>\r\n"]}]}