` }\"\r\n      @mousedown=\"startSwipe($event)\"\r\n      @touchstart=\"startSwipe($event)\"\r\n      @mousemove=\"swipe($event)\"\r\n      @touchmove=\"swipe($event)\"\r\n      @mouseup=\"endSwipe\"\r\n      @touchend=\"endSwipe\"\r\n    >\r\n      ef2\r\n    </div>\r\n    <div\r\n      class=\"card\"\r\n      :style=\"{ transform: `translateX(${cardX}px)` }\"\r\n      @mousedown=\"startSwipe($event)\"\r\n      @touchstart=\"startSwipe($event)\"\r\n      @mousemove=\"swipe($event)\"\r\n      @touchmove=\"swipe($event)\"\r\n      @mouseup=\"endSwipe\"\r\n      @touchend=\"endSwipe\"\r\n    >\r\n      ef3\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      startX: 0,\r\n      cardX: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    startSwipe(event) {\r\n      event.preventDefault();\r\n      this.startX = this.getClientX(event);\r\n    },\r\n    swipe(event) {\r\n      event.preventDefault();\r\n      if (this.startX) {\r\n        const currentX = this.getClientX(event);\r\n        const diffX = currentX - this.startX;\r\n        this.cardX = diffX;\r\n      }\r\n    },\r\n    endSwipe() {\r\n      this.startX = 0;\r\n      this.cardX = 0;\r\n    },\r\n    getClientX(event) {\r\n      return event.type.includes('mouse') ? event.clientX : event.touches[0].clientX;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.card-container {\r\n  width: 100%;\r\n  /* display: flex; */\r\n  justify-content: center;\r\n  align-items: center;\r\n  height: 100vh;\r\n}\r\n\r\n.card {\r\n  width: 100%;\r\n  height: 100vh;\r\n  background-color: #e0e0e0;\r\n  border-radius: 8px;\r\n  transition: transform 0.3s ease;\r\n  cursor: pointer;\r\n}\r\n</style>"]}]}