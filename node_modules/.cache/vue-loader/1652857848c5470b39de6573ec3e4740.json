v class=\"search-header\">\r\n          <div class=\"search-input\">\r\n            <input placeholder=\"search...\" v-model=\"search\" @keyup=\"searchLeads()\">\r\n            <div>\r\n              <img src=\"/images/search2.png\">\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        \r\n        <div class=\"card p-3\" v-for=\"lead in leads\" @click=\"$router.push('/lead/'+lead.id)\" v-if=\"(category_ids.length==0 || category_ids.includes(lead.category)) && (current_status=='Status' || lead.lead_status==current_status)\"> \r\n          <div class=\"d-flex justify-content-between pb-2\">\r\n            <div class=\"d-flex g3\">\r\n              <h6 class=\"text-primary\" style=\"font-size:0.75rem;\">{{lead.category_name}}</h6>\r\n            </div>\r\n            <div class=\"text-right\">\r\n              <h6 class=\"\" style=\"font-size:0.75rem;\" :style=\"'color:'+lead_statuse_color[lead.lead_status]\">{{lead.lead_status}}</h6>\r\n            </div>\r\n          </div>\r\n          <h4 style=\"color:#2d2d2d;font-weight: 600;\">{{lead.name}}</h4>\r\n          <h6 class=\"date pt-2 text-right\" style=\"color:#767676;white-space: nowrap;font-size:0.75rem;margin-bottom: -9px;\">{{timeAgo(lead.created_on)}}</h6>\r\n        </div>\r\n        <div v-if=\"is_loading\">\r\n            <div id=\"loading-bar-spinner\" class=\"spinner\"><div class=\"spinner-icon\"></div></div>\r\n        </div>\r\n        \r\n      </main>\r\n      <MainCrmFooter @categoryIds=\"categoryIds\" @currentStatus=\"currentStatus\"></MainCrmFooter>\r\n      \r\n    </div>\r\n</template>\r\n<script>\r\nimport MainHeader from '/src/views/app/pages/header'\r\nimport MainCrmFooter from '/src/views/app/pages/crm_footer'\r\nexport default {\r\n  metaInfo: {\r\n    title: \"Home\"\r\n  },\r\n  data() {\r\n    return {\r\n      background_start:true,\r\n      lastScrollPosition: 0,\r\n      lead_categories:[],\r\n      leads:[],\r\n      category_ids:[],\r\n      current_status:'Status',\r\n      search:'',\r\n      is_loading:false,\r\n    };\r\n  },\r\n   components:{\r\n      MainHeader,\r\n      MainCrmFooter,\r\n    },\r\n  mounted(){\r\n    var self = this\r\n    document.getElementsByClassName(\"lead-main\")[0].addEventListener(\"scroll\", () => {\r\n        self.handleScroll()\r\n    });\r\n    if(this.$route.query.auth_token){\r\n      this.$root.auth_token = this.$route.query.auth_token\r\n      localStorage.setItem(\"auth_token\",this.$root.auth_token)\r\n    }\r\n    if(this.$root.auth_token){\r\n      this.getUser()\r\n    }\r\n  },\r\n  methods: {\r\n    searchLeads() {\r\n      this.is_loading = true\r\n      var headers = new Headers();\r\n      if(this.search==''){\r\n        this.leads = []\r\n        this.is_loading = false\r\n        return false\r\n      }\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.auth_token);\r\n      var params = '?user_wise=yes&search='+this.search\r\n      fetch(this.api_url+'/employee/leads/'+params, {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.is_loading = false\r\n        this.leads = jsonData\r\n      })\r\n    },\r\n    categoryIds(data){\r\n      this.category_ids = data\r\n    },\r\n    currentStatus(data){\r\n      this.current_status = data\r\n    },\r\n    handleScroll () {\r\n      const currentScrollPosition = document.getElementsByClassName(\"lead-main\")[0]\r\n          .scrollTop\r\n      if (currentScrollPosition < 0) {\r\n        return 0\r\n      }\r\n      this.background_start = currentScrollPosition < this.lastScrollPosition\r\n      this.lastScrollPosition = currentScrollPosition\r\n    },\r\n   \r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n@import './style/style.css';\r\n\r\n\r\n</style>\r\n"]}]}