: {\r\n    title: \"Templates\"\r\n  },\r\n  components:{\r\n    MainHeader,\r\n  },\r\n  data() {\r\n    return {\r\n      campaign:{id:null, template:null},\r\n      templates:[],\r\n    };\r\n  },\r\n  mounted(){\r\n    this.getTemplates()\r\n    if(this.$route.params.campaign_id){\r\n      this.campaign.id = this.$route.params.campaign_id\r\n      this.getCampaign()\r\n    }\r\n  },\r\n  methods: {\r\n    getCampaign() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.auth_token);\r\n      fetch(this.api_url+'/employee/campaigns/'+this.campaign.id+'/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.campaign = jsonData\r\n      })\r\n    },\r\n    getTemplates() {\r\n      var headers = new Headers();\r\n      headers.append(\"Authorization\", \"Bearer \"+this.$root.auth_token);\r\n      fetch(this.api_url+'/employee/templates/', {\r\n          method : 'get',\r\n          headers: headers,\r\n      })\r\n      .then((response) => {\r\n          return response.json()\r\n      })\r\n      .then((jsonData) => {\r\n        this.templates = jsonData\r\n      })\r\n    },\r\n    selectTemplate(){\r\n      if(this.campaign.id){\r\n        this.$router.push('/edit-template/'+this.campaign.template+'/'+this.campaign.id)\r\n      }else{\r\n        this.$router.push('/edit-template/'+this.campaign.template)\r\n      }\r\n    },\r\n  }\r\n};\r\n</script>\r\n<style scoped>\r\n\r\n@import './style/style.css';\r\n  \r\n</style>\r\n"],"sourceRoot":"src/views/app/pages"}]}