A;;AANA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GAAA;;AAAA,kBAAA,UAAA;AAAA;AAAA;AAAA,GAAA;;AAUA,IAAA,eAAA;AAAA,sEAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBAEA,KAAA,CAAA,GAAA,CAAA,8CAAA,CAFA;;AAAA;AAEA,YAAA,QAFA;AAGA,YAAA,KAAA,CAAA,UAAA,GAAA,QAAA,CAAA,IAAA;AAHA;AAAA;;AAAA;AAAA;AAAA;AAKA,YAAA,OAAA,CAAA,KAAA,CAAA,4BAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GAAA;;AAAA,kBAAA,eAAA;AAAA;AAAA;AAAA,GAAA;;AASA,IAAA,gBAAA,GAAA,SAAA,gBAAA,CAAA,UAAA,EAAA;AACA,EAAA,KAAA,CAAA,gBAAA,GAAA,UAAA;;AACA,MAAA,UAAA,KAAA,IAAA,EAAA;AACA,IAAA,KAAA,CAAA,aAAA,GAAA,KAAA,CAAA,KAAA;AACA,GAFA,MAEA;AACA,IAAA,KAAA,CAAA,aAAA,GAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,aAAA,IAAA,CAAA,QAAA,CAAA,EAAA,KAAA,UAAA;AAAA,KAAA,CAAA;AACA;AACA,CAPA;;AASA,IAAA,YAAA,GAAA,SAAA,YAAA,GAAA;AACA,MAAA,YAAA,GAAA,IAAA,IAAA,CAAA,KAAA,CAAA,YAAA,EAAA,WAAA,GAAA,KAAA,CAAA,GAAA,EAAA,CAAA,CAAA;;AACA,MAAA,YAAA,KAAA,EAAA,EAAA;AACA,IAAA,KAAA,CAAA,aAAA,GAAA,KAAA,CAAA,KAAA;AACA,GAFA,MAEA;AACA,IAAA,KAAA,CAAA,aAAA,GAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,aAAA,IAAA,CAAA,IAAA,CAAA,KAAA,CAAA,GAAA,EAAA,CAAA,MAAA,YAAA;AAAA,KAAA,CAAA;AACA;AACA,CAPA;;AASA,SAAA,CAAA,YAAA;AACA,EAAA,UAAA;AACA,EAAA,eAAA;AACA,CAHA,CAAA","sourcesContent":["<script setup>\r\nimport axios from 'axios';\r\nimport { reactive, onMounted } from 'vue';\r\n\r\nconst state = reactive({\r\n  blogs: [],\r\n  categories: [],\r\n  selectedCategory: null,\r\n  selectedDate: '',\r\n  filteredBlogs: []\r\n});\r\n\r\nconst fetchBlogs = async () => {\r\n  try {\r\n    const response = await axios.get('http://127.0.0.1:8000/blogs/?format=api');\r\n    state.blogs = response.data;\r\n    state.filteredBlogs = response.data;\r\n  } catch (error) {\r\n    console.error('Error fetching blogs:', error);\r\n  }\r\n};\r\n\r\nconst fetchCategories = async () => {\r\n  try {\r\n    const response = await axios.get('http://127.0.0.1:8000/categories/?format=api');\r\n    state.categories = response.data;\r\n  } catch (error) {\r\n    console.error('Error fetching categories:', error);\r\n  }\r\n};\r\n\r\nconst filterByCategory = (categoryId) => {\r\n  state.selectedCategory = categoryId;\r\n  if (categoryId === null) {\r\n    state.filteredBlogs = state.blogs;\r\n  } else {\r\n    state.filteredBlogs = state.blogs.filter((blog) => blog.category.id === categoryId);\r\n  }\r\n};\r\n\r\nconst filterByDate = () => {\r\n  const selectedDate = new Date(state.selectedDate).toISOString().split('T')[0];\r\n  if (selectedDate === '') {\r\n    state.filteredBlogs = state.blogs;\r\n  } else {\r\n    state.filteredBlogs = state.blogs.filter((blog) => blog.date.split('T')[0] === selectedDate);\r\n  }\r\n};\r\n\r\nonMounted(() => {\r\n  fetchBlogs();\r\n  fetchCategories();\r\n});\r\n</script>\r\n\r\n<template>\r\n  <div>\r\n    <h1>Blog List</h1>\r\n\r\n    <!-- Filter buttons -->\r\n    <div>\r\n      <button @click=\"filterByCategory(null)\">All</button>\r\n      <button v-for=\"category in state.categories\" :key=\"category.id\" @click=\"filterByCategory(category.id)\">\r\n        {{ category.name }}\r\n      </button>\r\n    </div>\r\n\r\n    <!-- Filter by date -->\r\n    <input type=\"date\" v-model=\"state.selectedDate\" @input=\"filterByDate\" />\r\n\r\n    <!-- Blogs cards -->\r\n    <div class=\"blog-cards\">\r\n      <div v-for=\"blog in state.filteredBlogs\" :key=\"blog.id\" class=\"blog-card\">\r\n        <h2>{{ blog.name }}</h2>\r\n        <p>{{ blog.description }}</p>\r\n        <img :src=\"blog.image\" alt=\"Blog Image\">\r\n        <p>Category: {{ blog.category.name }}</p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n"],"sourceRoot":"src/views/app/pages"}]}